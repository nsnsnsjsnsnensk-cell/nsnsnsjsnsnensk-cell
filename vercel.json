{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/owner/create",
      "dest": "/owner/create/index.html"
    },
    {
      "src": "/owner/create/(.*)",
      "dest": "/owner/create/$1"
    },
    {
      "src": "/user/reseller",
      "dest": "/user/reseller/index.html"
    },
    {
      "src": "/user/reseller/(.*)",
      "dest": "/user/reseller/$1"
    },
    {
      "src": "/admin/dashboard",
      "dest": "/admin/dashboard/index.html"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
